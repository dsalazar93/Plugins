			margin 0
			list-style none
			padding 0

			li
				background-color #eee
				border thin solid #ccc
				margin 1em 0
				padding 1em
				width 100%

				
	.lnt-dnd-right
		ul
			margin 0
			list-style none
			padding 0
			
			li
				background-color #eee
